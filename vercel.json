{
  "version": 2,
  "functions": {
    "api/polymarket.js": {
      "runtime": "@vercel/node"
    },
    "api/polymarket-trades.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/polymarket(.*)",
      "dest": "/api/polymarket.js"
    },
    {
      "src": "/api/polymarket-trades(.*)",
      "dest": "/api/polymarket-trades.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
